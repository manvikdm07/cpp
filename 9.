#include <iostream>
using namespace std;

void swap(int *x, int *y) {
    int temp;
    temp = *x;
    *x = *y;
    *y = temp;
}

int main() {
    int a = 10, b = 20;

    cout << "Before swapping: " << a << " " << b << endl;

    swap(&a, &b);   // passing addresses

    cout << "After swapping: " << a << " " << b << endl;

    return 0;
}
